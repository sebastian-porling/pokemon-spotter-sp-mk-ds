    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "firebase": "^7.19.1",
    "firebase-admin": "^9.1.1",
    "internal-ip": "^6.1.0",
    "jsonfile": "^6.0.1",
    "mongodb": "^3.6.0",
    "monk": "^7.3.2",
